<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.hanbing.demo"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".md.ScrollingActivity"
            android:label="@string/title_activity_scrolling"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".md.ScrollingActivity2"
            android:label="@string/title_activity_scrolling2"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".EmptyActivity">
        </activity>
        <activity android:name=".activity.TestOnActivityResultActivity">
        </activity>
        <activity android:name=".activity.TestNumberPickerActivity">
        </activity>
        <activity android:name=".activity.TestRestoreActivity">
        </activity>
        <activity android:name=".activity.TestSlideMenuActivity">
        </activity>
        <activity android:name=".activity.TestSlideMenu2Activity">
        </activity>
    </application>

</manifest>